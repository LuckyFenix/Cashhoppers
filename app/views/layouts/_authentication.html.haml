-unless current_user
  .non-print{style: 'margin-top: 7%'}
    %a{href: '/auth/facebook', class: 'services'}
      = image_tag 'btn_login_facebook.png', size: '165x64', alt: 'Facebook'
    %a{href: '/auth/twitter', class: 'services'}
      = image_tag 'btn_login_twitter.png', size: '165x64', alt: 'Twitter'
    %a{href: '/auth/google', class: 'services'}
      = image_tag 'btn_login_google.png', size: '165x64', alt: 'Twitter'
