<h1>Listing admin_applications</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Api key</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_applications.each do |admin_application| %>
  <tr>
    <td><%= admin_application.name %></td>
    <td><%= admin_application.description %></td>
    <td><%= admin_application.api_key %></td>
    <td><%= link_to 'Show', admin_application_path(admin_application) %></td>
    <td><%= link_to 'Edit', edit_admin_application_path(admin_application) %></td>
    <td><%= link_to 'Destroy', admin_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Application', new_admin_application_path %>
